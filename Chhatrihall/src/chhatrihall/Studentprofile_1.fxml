<AnchorPane id="AnchorPane" prefHeight="580.0" prefWidth="799.0" stylesheets="@studentprofile.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chhatrihall.HomeController">
   <children>
      <ImageView fitHeight="580.0" fitWidth="799.0" pickOnBounds="true">
         <image>
            <Image url="@studentprofile.jpg" />
