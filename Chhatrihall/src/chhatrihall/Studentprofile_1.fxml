      <Label alignment="CENTER" layoutX="270.0" layoutY="103.0" prefHeight="53.0" prefWidth="104.0" style="-fx-border-radius: 10; -fx-background-radius: 10;" text="Student ID :">
         <font>
            <Font name="Book Antiqua Bold" size="14.0" />
         </font></Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="270.0" layoutY="153.0" prefHeight="53.0" prefWidth="104.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Student Name :">
         <font>
            <Font name="Book Antiqua Bold" size="14.0" />
         </font></Label>
      <Label alignment="CENTER" layoutX="270.0" layoutY="203.0" prefHeight="53.0" prefWidth="96.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Level :">
